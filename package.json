{
  "name": "austin-site",
  "version": "0.1.0",
  "homepage": "https://atb-brown.github.io/austin",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/babel__generator": "^7.6.4",
    "@types/jest": "^29.5.0",
    "@types/node": "^16.18.21",
    "@types/react": "^18.0.30",
    "@types/react-dom": "^18.0.11",
    "axios": "^1.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "lint": "eslint src --ext .ts,.tsx,.js",
    "prettyCheck": "prettier --check .",
    "pretty": "prettier --write .",
    "sca": "yarn prettyCheck && yarn tsc && yarn lint && yarn audit",
    "siteBuild": "react-scripts build",
    "siteDeploy": "gh-pages -d build",
    "siteEject": "react-scripts eject",
    "sitePredeploy": "npm run build",
    "siteStart": "react-scripts start",
    "siteStartDev": "DISABLE_ESLINT_PLUGIN=true yarn siteStart",
    "test": "jest",
    "verify": "yarn sca && yarn test --coverage"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.38.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^5.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "prettier-plugin-css-order": "^2.0.1",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^29.0.5"
  },
  "resolutions": {
    "nth-check": "^2.0.1",
    "postcss": "^8.4.31"
  }
}
